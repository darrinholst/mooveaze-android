<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.google.mooveaze"
          android:versionCode="1"
          android:versionName="1.0">
    <application android:label="mooveaze" android:icon="@drawable/icon" android:name="Mooveaze">
        <activity android:name=".controller.MoviesActivity" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".controller.KiosksActivity"/>
        <activity android:name=".controller.GenresActivity"/>
    </application>
    <uses-sdk android:minSdkVersion="7"/>
    <uses-permission android:name="android.permission.INTERNET"/>
</manifest>
